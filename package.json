{
  "name": "vscode-devtools",
  "displayName": "DevTools for Chrome",
  "description": "Open the chrome devtools as a dockable webview inside vs code.",
  "version": "0.0.1",
  "preview": true,
  "license": "SEE LICENSE IN LICENSE",
  "publisher": "jalissiak",
  "icon": "icon.png",
  "homepage": "https://github.com/CodeMooseUS/vscode-devtools/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/CodeMooseUS/vscode-devtools"
  },
  "bugs": {
    "url": "https://github.com/CodeMooseUS/vscode-devtools/issues"
  },
  "galleryBanner" : {
		"color": "#5c2d91",
		"theme" : "dark"
	},
  "engines": {
    "vscode": "^1.25.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:devtools.attach",
    "onCommand:devtools.launch",
    "onWebviewPanel:devtools"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "devtools.attach",
        "title": "Attach to a target",
        "category": "DevTools"
      },
      {
        "command": "devtools.launch",
        "title": "Launch Chrome and the Chrome DevTools",
        "category": "DevTools"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "dependencies": {
    "chrome-devtools-frontend": "^1.0.602557",
    "vscode": "^1.1.18",
    "ws": "^6.1.0"
  },
  "devDependencies": {
    "@types/node": "^10.5.2",
    "@types/ws": "^6.0.1",
    "typescript": "^3.1.1"
  }
}
